>\r\n        </v-list>\r\n      </v-menu>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n/* eslint-disable */\r\n// eslint-disable-next-line\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Searche\",\r\n\r\n  data: () => ({\r\n    searchString: \"\",\r\n    postList: [],\r\n    closeOnClick: false,\r\n    find: \"Find in records \",\r\n    newRecord: \"Add New Record\",\r\n  }),\r\n  methods: {\r\n    getData() {\r\n      axios.get(\"mockData.json\").then((response) => {\r\n        const list = response.data.data;\r\n        for (let i = 0; i < list.length; i++) {\r\n          const x = list[i];\r\n\r\n          this.postList.push({\r\n            name: x[0],\r\n            company: x[1],\r\n            email: x[2],\r\n            date: x[3],\r\n            country: x[4],\r\n            city: x[5],\r\n          });\r\n        }\r\n        // class Post {\r\n        //   constructor(title, link, author, img) {\r\n        //     this.title = title;\r\n        //     this.link = link;\r\n        //     this.author = author;\r\n        //     this.img = img;\r\n        //   }\r\n        // }\r\n      });\r\n    },\r\n    goList() {\r\n      this.$router.push({\r\n        path: \"/list\",\r\n      });\r\n      this.$store.commit(\"setSearcheString\", this.searchString);\r\n    },\r\n  },\r\n  computed: {\r\n    filteredData() {\r\n      var self = this;\r\n      self.postList = self.postList.filter((customer) => {\r\n        if (\r\n          customer.name\r\n            .toLowerCase()\r\n            .indexOf(self.searchString.toLowerCase()) >= 0 ||\r\n          customer.company\r\n            .toLowerCase()\r\n            .indexOf(self.searchString.toLowerCase()) >= 0\r\n        )\r\n          return customer;\r\n      });\r\n\r\n      return self.postList;\r\n    },\r\n    columns() {\r\n      if (this.$vuetify.breakpoint.xl) {\r\n        return 4;\r\n      }\r\n\r\n      if (this.$vuetify.breakpoint.lg) {\r\n        return 3;\r\n      }\r\n\r\n      if (this.$vuetify.breakpoint.md) {\r\n        return 2;\r\n      }\r\n\r\n      return 1;\r\n    },\r\n  },\r\n  created() {\r\n    axios.get(\"mockData.json\").then((response) => {\r\n      const list = response.data.data;\r\n      for (let i = 0; i < list.length; i++) {\r\n        const x = list[i];\r\n\r\n        this.postList.push({\r\n          name: x[0],\r\n          company: x[1],\r\n          email: x[2],\r\n          date: x[3],\r\n          country: x[4],\r\n          city: x[5],\r\n        });\r\n      }\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n"]},"metadata":{},"sourceType":"module"}